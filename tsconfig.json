{
  "compilerOptions": {
    "module": "esnext",
    "target": "es2017",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "out",
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "paths": {
      "*": [
        "node_modules/*"
      ]
    }
  },
  "exclude": [
    "./node_modules/*",
    "logs/**/*"
  ]
}
