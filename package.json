{
  "name": "koa-svr",
  "version": "1.0.0",
  "description": "koa server framework",
  "main": "index.ts",
  "scripts": {
    "start": "tsc-watch --onSuccess \"nodemon out/index.js\"",
    "dev": "tsc-watch --onSuccess \"nodemon --inspect out/index.js\"",
    "update": "git pull && npm i && tsc",
    "deploy": "npm run update && pm2 restart pm2.config.js",
    "lint": "eslint -c .eslintrc **/*.ts --fix",
    "rmlog": "rm logs/*/* -rf"
  },
  "keywords": [],
  "author": "Dobility",
  "license": "ISC",
  "devDependencies": {
    "@types/koa": "^2.0.49",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-router": "^7.0.42",
    "@types/mongoose": "^5.5.17",
    "@types/node": "^12.7.5",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mongoose": "^5.7.1",
    "nodemon": "^1.19.2",
    "pm2": "^3.5.1",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "koa": "^2.8.1",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^5.1.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "log4js": "^5.1.0",
    "saslprep": "^1.0.3"
  }
}
